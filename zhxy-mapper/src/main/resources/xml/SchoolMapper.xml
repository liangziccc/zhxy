<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.etoak.zhxy.jcxx.mapper.SchoolMapper">
    <select id="queryAll" resultType="com.etoak.zhxy.jcxx.entity.School">
        select * from  school
    </select>

    <select id="querySchById" resultType="com.etoak.zhxy.jcxx.entity.School">
        select * from school where id =#{id}
    </select>
</mapper>